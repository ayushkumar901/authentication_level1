<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="stylesheets/style.css">
</head>
<body>
    <nav>
        <a href="http://localhost:3000/">Home</a>
        <a href="http://localhost:3000/form">Create form</a>
    </nav>
   <h1>File Name</h1>
   <div class="page">
    <% arr.forEach((val) => { %>
        <div class="box">
            <h3>
                <%= val.filename %>          
            </h3> 
            <p>
                <%= val.data %>
            </p>

            <a class="edit" href="/edit/<%= val.filename %>">edit</a>
            <a class="delete" href="/delete/<%= val.filename %>">delete</a>
        </div>
    <% }); %>
   </div>
</body>
</html>